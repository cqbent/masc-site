{#
/**
 * @file
 * Theme override for a 'button' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - value: input value
 *
 * @see template_preprocess_input()
 */
#}
<button{{ attributes }}>
  <span>{{ attributes.value }}</span>
</button>